{
  "name": "stock-analysier-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create stock-analysier-db",
    "db:migrate:prod": "wrangler d1 execute stock-analysier-db --remote --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute stock-analysier-db --local --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^4.11.9",
    "jose": "^6.1.3",
    "zod": "^4.3.6",
    "@cloudflare/workers-types": "^4.20240129.0"
  },
  "devDependencies": {
    "wrangler": "^3.22.0",
    "typescript": "^5.3.0"
  }
}
